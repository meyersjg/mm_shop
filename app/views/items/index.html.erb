<div class="container">

  <div class="dropcart">

  </div>

  <% @items.each do |item| %>
    <div class="col4">
      <div class="item_pic">

        <%= image_tag("/assets/products/#{item.picture}") %>
        <h3><%= item.name %></h3>
        <p>$<%= item.cost %> </p>

      </div>

    </div>

  <% end %>

</div>
